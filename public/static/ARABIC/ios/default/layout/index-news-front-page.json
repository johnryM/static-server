                      {"$":"title", "title":"layoutfiletitle.thepapers"},
                      {"$":"featureGroup", "secondary":"$st_in_the_papers", "limit":1, "mode==portrait":{"imageAspect":0.28125}}
                      ],
                "$2":[
                      {"$":"title", "title":"layoutfiletitle.thereporters", "contentLink": "/cps/news/the_reporters?title=The%20Reporters"},
