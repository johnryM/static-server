                {"$":"featuresSection",   "modes":["tablet"],  "$secondary":"$st_features"},
                
                
                {"$":"twoDigestGroups", "$1":"$digestBusinessUk",      "$2":"$digestPoliticsUk"},
                {"$":"twoDigestGroups", "$1":"$digestHealthUk",        "$2":"$digestEducationUk"},
                {"$":"twoDigestGroups", "$1":"$digestTechnologyUk",    "$2":"$digestScienceUk"},
                {"$":"twoDigestGroups", "$1":"$digestEntertainmentUk", "$2":"$digestMagazineUk"},
                
                {"$":"digestTitle", "title": "Sport", "contentLink": "/cps/sport/front-page?title=Sport"},
