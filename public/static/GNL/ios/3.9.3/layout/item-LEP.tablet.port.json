{
	"format": ["bbc.mobile.news.format.liveevent"],
	"modes": ["tablet","portrait"],
	"class": "BNVerticalStack",
	"background": "#fff",
    "modules": [
		  {
			"cellClass": "BNCellMedia",
			"limit": 1,
			"offset": 0,
			"primary": ["bbc.mobile.news.image","bbc.mobile.news.video","bbc.mobile.news.audio"],
			"secondary": ["bbc.mobile.news.placement.lmp"],
			"filterIsAvailable": "yes",
			"parallax": "yes",
			"imageAspect": 0.5625,
			"inverseColorScheme": "yes"
		  },
		  {
			"cellClass": "BNCellHeadline",
			"textPadding": "{12, 64, 4, 64}",
			"background": "#1e1e1e",
			"inverseColorScheme": "yes"
		  },
		  {
			"class": "BNKeyPointsModule",
			"background": "#1e1e1e",
			"textPadding": "{4, 64, 12, 64}"
		  },
		  {
			"class": "BNCommentaryModule",
			"textPadding": "{0, 64, 0, 64}",
			"contentKey": "commentaryId"
		 },
        {
        "class":"BNCopyrightModule",
        "padding": "{8,64,8,64}"
        }
    ]
}
