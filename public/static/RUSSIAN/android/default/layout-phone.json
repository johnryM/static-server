{
  "phone-portrait-top-stories": {
    "apply-first": "cps_index_base",
    "filter": {
      "site": [
        "/russian"
      ],
      "content": [
        "/cps/russian/front_page"
      ],
      "format": [
        "bbc.mobile.news.cps.idx"
      ],
      "orientation": [
        "portrait"
      ]
    },
    "modules": [
      {
        "id": "top-story",
        "presenter": "top-story",
        "filter": {
          "primary": [
            "bbc.mobile.news.item"
          ],
          "offset": 0,
          "limit": 1,
          "secondary": [
            "bbc.mobile.news.group.top-story",
            "bbc.mobile.news.group.more-top-stories",
            "bbc.mobile.news.group.top-stories-overflow"
          ]
        },
        "config": {
          "script-top": "true"
        },
        "presenter~compact": "1-story-per-row",
        "config~compact": {
          "topPadding": true
        }
      },
      {
        "id": "stories",
        "presenter": "1-story-per-row",
        "filter": {
          "primary": [
            "bbc.mobile.news.item"
          ],
          "offset": 1,
          "limit": 4,
          "secondary": [
            "bbc.mobile.news.group.top-story",
            "bbc.mobile.news.group.more-top-stories",
            "bbc.mobile.news.group.top-stories-overflow"
          ]
        },
        "config": {
          "script-below": "top-story"
        }
      },
      {
        "id": "latest-news",
        "config": {
          "script-below": "stories"
        },
        "filter": {
          "secondary": [
            "bbc.mobile.news.group.latest-news"
          ],
          "offset": 0,
          "limit": 3
        },
        "presenter": "1-story-per-row"
      },
      {
        "id": "stories-2",
        "config": {
          "script-below": "latest-news"
        },
        "filter": {
          "secondary": [
            "bbc.mobile.news.group.top-story",
            "bbc.mobile.news.group.more-top-stories",
            "bbc.mobile.news.group.top-stories-overflow"
          ],
          "offset": 5,
          "limit": -1
        },
        "presenter": "1-story-per-row"
      },
      {
        "id": "av-story-carousel",
        "presenter": "av-story-carousel",
        "filter": {
          "subFilters": [

          ],
          "primary": [
            "bbc.mobile.news.item"
          ],
          "secondary": [
            "bbc.mobile.news.group.watch-listen",
            "bbc.mobile.news.group.av-stories-live"
          ],
          "offset": 0,
          "limit": -1
        },
        "config": {
          "script-below": "stories-2"
        }
      },
      {
        "id": "press-review",
        "config": {
          "script-below": "av-story-carousel",
          "title": "Press Review",
          "title-id": "layout_press_review"
        },
        "filter": {
          "primary": [
            "bbc.mobile.news.item"
          ],
          "secondary": [
            "bbc.mobile.news.group.the-papers"
          ],
          "offset": 0,
          "limit": -1
        },
        "presenter": "1-story-per-row"
      },
      {
        "id": "also-in-the-news",
        "config": {
          "script-below": "press-review",
          "title": "Also in the News",
          "title-id": "layout_also_in_the_news"
        },
        "filter": {
          "primary": [
            "bbc.mobile.news.item"
          ],
          "secondary": [
            "bbc.mobile.news.group.also-in-the-news"
          ],
          "offset": 0,
          "limit": 2
        },
        "presenter": "1-story-per-row"
      },
      {
        "id": "journal",
        "config": {
          "script-below": "also-in-the-news",
          "title": "Journal",
          "title-id": "layout_journal",
          "style": "featured-large-phone"
        },
        "filter": {
          "primary": [
            "bbc.mobile.news.item"
          ],
          "secondary": [
            "bbc.mobile.news.group.verticals"
          ],
          "offset": 0,
          "limit": -1
        },
        "presenter": "1-story-per-row"
      },
      {
        "id": "blogs",
        "config": {
          "script-below": "journal",
          "title": "Blogs",
          "title-id": "layout_blogs",
          "style": "digest"
        },
        "filter": {
          "primary": [
            "bbc.mobile.news.item"
          ],
          "secondary": [
            "bbc.mobile.news.group.blogs"
          ],
          "offset": 0,
          "limit": 3
        },
        "presenter": "1-story-per-row"
      },
      {
        "id": "features-and-analysis-2",
        "config": {
          "script-below": "blogs",
          "title": "Features & Analysis",
          "title-id": "layout_features_analysis",
          "style": "featured-large-phone"
        },
        "filter": {
          "primary": [
            "bbc.mobile.news.item"
          ],
          "secondary": [
            "bbc.mobile.news.group.special-reports-2"
          ],
          "offset": 0,
          "limit": 1
        },
        "presenter": "1-story-per-row",
        "config~compact": {
          "title": "Features & Analysis",
          "title-id": "layout_features_analysis",
          "summaryOnly": true
        }
      },
      {
        "id": "editor-choice",
        "config": {
          "script-below": "features-and-analysis-2",
          "title": "Editor's choice",
          "title-id": "layout_editor_choice"
        },
        "filter": {
          "primary": [
            "bbc.mobile.news.item"
          ],
          "secondary": [
            "bbc.mobile.news.group.features"
          ],
          "offset": 0,
          "limit": 10
        },
        "presenter": "1-story-per-row"
      },
      {
        "id": "in-pictures",
        "config": {
          "script-below": "editor-choice",
          "style": "photo-gallery",
          "title": "In Pictures",
          "title-id": "layout_in_pictures"
        },
        "filter": {
          "primary": [
            "bbc.mobile.news.item"
          ],
          "secondary": [
            "bbc.mobile.news.group.picture-galleries"
          ],
          "offset": 0,
          "limit": 4
        },
        "presenter": "1-story-per-row"
      },
      {
        "id": "more-stories",
        "presenter": "1-story-per-row"
      },
      {
        "id": "copyright",
        "presenter": "copyright"
      }
    ]
  },
